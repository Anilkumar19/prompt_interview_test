<p id="notice"><%= notice %></p>

<h1>Listing Organizations</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>Org Name</th>
      <th>Org Identifier</th>
      <th>Created_by</th>
      <th>Country</th>
      <th>Created_at</th>
      <th>Updated_at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.id %></td> 
        <td><%= organization.name %></td> org_identifier
        <td><%= organization.org_identifier %></td>
        <td><%= organization.user.first_name + " " + organization.user.last_name%></td>
        <td><%= organization.country %></td>
        <td><%= organization.created_at %></td>
        <td><%= organization.updated_at %></td>
       <!--  <td><%= link_to 'Show', organization %></td>
        <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
        <td><%= link_to 'Destroy', organization, method: :delete, data: { confirm: 'Are you sure?' } %> --></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @organizations %>


